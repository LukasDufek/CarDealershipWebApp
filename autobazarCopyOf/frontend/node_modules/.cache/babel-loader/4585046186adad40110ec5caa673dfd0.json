{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport axios from \"axios\";\nexport default {\n  name: 'carSelection',\n\n  /*\r\n  props:{\r\n    brand:{\r\n      type: String\r\n    }\r\n   }\r\n   */\n  data() {\n    return {\n      Cars: [],\n      //car: {},\n      vybranaAuta: [],\n      zvoleneAuto: {},\n      minCena: 0,\n      maxCena: 10000000,\n      znacky: [],\n      vybraneZnacky: [],\n      vsechnyZnacky: true,\n      karoserie: [],\n      vybraneKaroserie: [],\n      vsechnyKaroserie: true\n    };\n  },\n\n  mounted() {\n    axios.get('http://localhost:5000/api/posts/').then(response => {\n      //console.log(response.data);\n      this.Cars = response.data;\n      this.getznacky();\n      this.getCaroseries();\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    vypisMinCenu(cena) {\n      this.minCena = cena;\n      console.log('Min cena je ', this.minCena);\n    },\n\n    vypisMaxCenu(cena) {\n      this.minCena = cena;\n      console.log('Max cena je ', this.maxCena);\n    },\n\n    //is selected car allready in array of vybranaAuta\n    isInArray(id) {\n      let bool = false;\n\n      for (let i = 0; i < this.vybranaAuta.length; i++) {\n        if (this.vybranaAuta[i]._id === id) {\n          bool = true;\n        }\n      }\n\n      return bool;\n    },\n\n    getznacky() {\n      this.vybraneZnacky = [];\n\n      for (let i = 0; i < this.Cars.length; i++) {\n        if (!this.znacky.includes(this.Cars[i].znacka)) this.znacky.push(this.Cars[i].znacka);\n      }\n    },\n\n    getCaroseries() {\n      this.vybraneKaroserie = [];\n\n      for (let i = 0; i < this.Cars.length; i++) {\n        if (!this.karoserie.includes(this.Cars[i].karoserie)) this.karoserie.push(this.Cars[i].karoserie);\n      }\n    },\n\n    addvybraneZnacky(brand) {\n      if (!this.vybraneZnacky.includes(brand)) {\n        this.vybraneZnacky.push(brand);\n        this.vsechnyZnacky = false;\n        console.log('Size of array' + this.vybraneZnacky.length);\n        console.log('Add brand: ' + this.vybraneZnacky[0]);\n      } else {\n        let index = this.vybraneZnacky.indexOf(brand);\n        this.vybraneZnacky.splice(index, 1);\n\n        if (this.vybraneZnacky.length === 0) {\n          this.vsechnyZnacky = true;\n        }\n      }\n\n      this.filtering();\n    },\n\n    pridatVybranouKaroserii(karoserie) {\n      if (!this.vybraneKaroserie.includes(karoserie)) {\n        this.vybraneKaroserie.push(karoserie);\n        this.vsechnyKaroserie = false;\n      } else {\n        let index = this.vybraneKaroserie.indexOf(karoserie);\n        this.vybraneKaroserie.splice(index, 1);\n\n        if (this.vybraneKaroserie.length === 0) {\n          this.vsechnyKaroserie = true;\n        }\n      }\n\n      this.filtering();\n    },\n\n    filtering() {\n      if (!this.vsechnyZnacky) {\n        for (let i = 0; i < this.vybranaAuta.length; i++) {\n          if (!this.vybraneZnacky.includes(this.vybranaAuta[i].znacka)) {\n            this.vybranaAuta.splice(i, 1);\n            i = -1;\n          }\n        }\n\n        for (let i = 0; i < this.Cars.length; i++) {\n          if (this.vybraneZnacky.includes(this.Cars[i].znacka) && !this.isInArray(this.Cars[i]._id)) {\n            this.vybranaAuta.push(this.Cars[i]);\n          }\n        }\n      }\n\n      if (!this.vybraneKaroserie) {\n        for (let i = 0; i < this.vybranaAuta.length; i++) {\n          if (!this.vybraneKaroserie.includes(this.vybranaAuta[i].karoserie)) {\n            this.vybranaAuta.splice(i, 1);\n            i = -1;\n          }\n        }\n\n        for (let i = 0; i < this.Cars.length; i++) {\n          if (this.vybraneKaroserie.includes(this.Cars[i].karoserie) && !this.isInArray(this.Cars[i]._id)) {\n            this.vybranaAuta.push(this.Cars[i]);\n          }\n        }\n      }\n    },\n\n    toCarDetail(car) {\n      this.zvoleneAuto = car;\n      this.$router.push('./carDetail');\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA8FA;AAEA;EACAA,oBADA;;EAEA;AACA;AACA;AACA;AACA;AACA;AACA;EAEAC;IACA;MACAC,QADA;MAEA;MACAC,eAHA;MAIAC,eAJA;MAKAC,UALA;MAMAC,iBANA;MAQAC,UARA;MASAC,iBATA;MAUAC,mBAVA;MAYAC,aAZA;MAaAC,oBAbA;MAcAC;IAdA;EAiBA,CA5BA;;EA8BAC;IACAC,8CACAC,IADA,CACAC;MACA;MACA;MACA;MACA;IAEA,CAPA,EAQAC,KARA,CAQAC;MACAC;IACA,CAVA;EAWA,CA1CA;;EA4CAC;IAEAC;MACA;MACAF;IAGA,CAPA;;IAQAG;MACA;MACAH;IACA,CAXA;;IAcA;IACAI;MACA;;MACA;QACA;UACAC;QACA;MACA;;MACA;IACA,CAvBA;;IAyBAC;MACA;;MACA;QACA,gDACA;MACA;IACA,CA/BA;;IAgCAC;MACA;;MACA;QACA,sDACA;MACA;IACA,CAtCA;;IAwCAC;MACA;QACA;QACA;QACAR;QACAA;MACA,CALA,MAKA;QACA;QACA;;QACA;UACA;QACA;MAEA;;MACA;IACA,CAvDA;;IAwDAS;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;;QACA;UACA;QACA;MAEA;;MACA;IACA,CArEA;;IAwEAC;MACA;QACA;UACA;YACA;YACAC;UACA;QACA;;QAEA;UACA;YACA;UACA;QACA;MACA;;MAEA;QACA;UACA;YACA;YACAA;UACA;QACA;;QAIA;UACA;YACA;UACA;QACA;MACA;IAGA,CA1GA;;IA6GAC;MACA;MACA;IAEA;;EAjHA;AA5CA","names":["name","data","Cars","vybranaAuta","zvoleneAuto","minCena","maxCena","znacky","vybraneZnacky","vsechnyZnacky","karoserie","vybraneKaroserie","vsechnyKaroserie","mounted","axios","then","response","catch","error","console","methods","vypisMinCenu","vypisMaxCenu","isInArray","bool","getznacky","getCaroseries","addvybraneZnacky","pridatVybranouKaroserii","filtering","i","toCarDetail"],"sourceRoot":"src/views","sources":["carsSelection.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <header-page/>\r\n    <section class=\"filtering-tag\">\r\n\r\n\r\n      <div  class=\"filter-znacky\" v-for=\"brand in znacky\" :key=\"brand._id\">\r\n        <input name=\"active\" type=\"checkbox\" :id=\"brand\" @change=\"addvybraneZnacky(brand)\">\r\n        <h3>{{ brand }}</h3>\r\n      </div>\r\n      <p>vybrane znacky: {{vybraneZnacky}}</p>\r\n\r\n\r\n      <div  class=\"filter-znacky\" v-for=\"karo in karoserie\" :key=\"karo._id\">\r\n        <input name=\"active\" type=\"checkbox\" :id=\"karo\" @change=\"pridatVybranouKaroserii(karo)\">\r\n        <h3>{{ karo }}</h3>\r\n      </div>\r\n      <p>vybrane karoserie: {{vybraneKaroserie}}</p>\r\n\r\n      <div  class=\"filter-znacky\">\r\n        <p>Od {{ message }}</p>\r\n        <input v-model.number=\"message\" value=\"vypisMinCenu(message)\" placeholder=\"Zadej minimální cenu\" />\r\n      </div>\r\n\r\n      <div  class=\"filter-znacky\">\r\n        <p>Od {{ message }}</p>\r\n        <input v-model.number=\"message\" value=\"vypisMaxCenu(message)\" placeholder=\"Zadej minimální cenu\" />\r\n      </div>\r\n\r\n\r\n\r\n\r\n\r\n    </section>\r\n\r\n    <section class=\"border-text\">\r\n      <form action=\"\">\r\n        <label>Seřadit:</label>\r\n        <select id=\"sorted\" name=\"sorted\">\r\n          <option value=\"nejlevnejsi\">Nejlevnější</option>\r\n          <option value=\"nejdrazsi\">Nejdražší</option>\r\n          <option value=\"nenovejsi\">Nejnovější</option>\r\n          <option value=\"tachometr\">Stav tachometru</option>\r\n        </select>\r\n        <input type=\"submit\">\r\n      </form>\r\n    </section>\r\n\r\n\r\n    <section class=\"optionalCar\" v-if=\"this.vsechnyZnacky && this.vsechnyKaroserie\">\r\n      <section class=\"border-text-choose-car\" v-for=\"car of Cars\"\r\n           :key=\"car._id\"\r\n      >\r\n        <div class=\"carDescription\">\r\n        <h1 class=\"subtitle-car-browsing\">{{car.znacka}} {{car.model}}</h1>\r\n        <img src=\"https://s4.gifyu.com/images/testImg.md.jpg\" class=\"imgCar\" alt=\"testImg.jpg\" border=\"0\" />\r\n        <h3 class=\"subtitle-car-browsing\">{{car.karoserie}}</h3>\r\n        <h3 class=\"subtitle-car-browsing\">{{car.motor}} {{car.vykon}} kW</h3>\r\n        <p>{{car.rok}}</p>\r\n        <br>\r\n          <button class=\"showCar\" @click=\"toCarDetail(car)\">Detail auta</button>\r\n        <br>\r\n        <h2 class =\"price-car-browsing\">{{car.cena}} Kč</h2>\r\n        </div>\r\n\r\n      </section>\r\n    </section>\r\n\r\n    <section class=\"optionalCar\" v-else>\r\n      <section class=\"border-text-choose-car\" v-for=\"car of vybranaAuta\"\r\n               :key=\"car._id\"\r\n      >\r\n        <img class=\"carImage\"/>\r\n        <h1 class=\"subtitle-car-browsing\">{{car.znacka}} {{car.model}}</h1>\r\n        <img src=\"https://s4.gifyu.com/images/testImg.md.jpg\" class=\"imgCar\" alt=\"testImg.jpg\" border=\"0\" />\r\n        <h3 class=\"subtitle-car-browsing\">{{car.karoserie}}</h3>\r\n        <h3 class=\"subtitle-car-browsing\">{{car.motor}} {{car.vykon}} kW</h3>\r\n        <p>{{car.rok}}</p>\r\n        <br>\r\n        <button class=\"showCar\" @click=\"toCarDetail(car)\">Detail auta</button>\r\n        <br>\r\n        <h2 class =\"price-car-browsing\">{{car.cena}} Kč</h2>\r\n\r\n      </section>\r\n    </section>\r\n\r\n  <footer-page/>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'carSelection',\r\n  /*\r\n  props:{\r\n    brand:{\r\n      type: String\r\n    }\r\n   }\r\n   */\r\n\r\n  data(){\r\n    return {\r\n      Cars: [],\r\n      //car: {},\r\n      vybranaAuta: [],\r\n      zvoleneAuto:{},\r\n      minCena: 0,\r\n      maxCena: 10000000,\r\n\r\n      znacky: [],\r\n      vybraneZnacky:[],\r\n      vsechnyZnacky: true,\r\n\r\n      karoserie:[],\r\n      vybraneKaroserie:[],\r\n      vsechnyKaroserie: true,\r\n\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    axios.get('http://localhost:5000/api/posts/')\r\n        .then((response) =>{\r\n          //console.log(response.data);\r\n          this.Cars = response.data\r\n          this.getznacky();\r\n          this.getCaroseries();\r\n\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n  },\r\n\r\n  methods: {\r\n\r\n    vypisMinCenu(cena){\r\n      this.minCena =cena;\r\n      console.log('Min cena je ',this.minCena);\r\n\r\n\r\n    },\r\n    vypisMaxCenu(cena){\r\n      this.minCena =cena;\r\n      console.log('Max cena je ',this.maxCena);\r\n    },\r\n\r\n\r\n    //is selected car allready in array of vybranaAuta\r\n    isInArray(id){\r\n      let bool = false;\r\n      for(let i=0; i< this.vybranaAuta.length; i++){\r\n        if(this.vybranaAuta[i]._id === id){\r\n          bool = true;\r\n        }\r\n      }\r\n      return bool;\r\n    },\r\n\r\n      getznacky(){\r\n        this.vybraneZnacky =[];\r\n        for(let i =0; i<this.Cars.length; i++){\r\n          if( !(this.znacky.includes(this.Cars[i].znacka)))\r\n              this.znacky.push(this.Cars[i].znacka);\r\n        }\r\n      },\r\n    getCaroseries(){\r\n      this.vybraneKaroserie =[];\r\n      for(let i =0; i<this.Cars.length; i++){\r\n        if( !(this.karoserie.includes(this.Cars[i].karoserie)))\r\n          this.karoserie.push(this.Cars[i].karoserie);\r\n      }\r\n    },\r\n\r\n    addvybraneZnacky(brand){\r\n        if(!this.vybraneZnacky.includes(brand)) {\r\n          this.vybraneZnacky.push(brand);\r\n          this.vsechnyZnacky =false;\r\n          console.log('Size of array'+this.vybraneZnacky.length)\r\n          console.log('Add brand: '+this.vybraneZnacky[0]);\r\n        }else{\r\n            let index = this.vybraneZnacky.indexOf(brand);\r\n            this.vybraneZnacky.splice(index, 1);\r\n            if (this.vybraneZnacky.length === 0) {\r\n              this.vsechnyZnacky = true;\r\n            }\r\n\r\n        }\r\n        this.filtering();\r\n    },\r\n    pridatVybranouKaroserii(karoserie){\r\n      if(!this.vybraneKaroserie.includes(karoserie)) {\r\n        this.vybraneKaroserie.push(karoserie);\r\n        this.vsechnyKaroserie =false;\r\n      }else{\r\n        let index = this.vybraneKaroserie.indexOf(karoserie);\r\n        this.vybraneKaroserie.splice(index, 1);\r\n        if (this.vybraneKaroserie.length === 0) {\r\n          this.vsechnyKaroserie = true;\r\n        }\r\n\r\n      }\r\n      this.filtering();\r\n    },\r\n\r\n\r\n    filtering(){\r\n        if(!this.vsechnyZnacky){\r\n          for(let i =0; i<this.vybranaAuta.length; i++){\r\n              if (!(this.vybraneZnacky.includes(this.vybranaAuta[i].znacka))) {\r\n                this.vybranaAuta.splice(i, 1);\r\n                i =-1\r\n            }\r\n        }\r\n\r\n          for(let i =0; i<this.Cars.length; i++) {\r\n              if (this.vybraneZnacky.includes(this.Cars[i].znacka) && (!this.isInArray(this.Cars[i]._id)) ) {\r\n                this.vybranaAuta.push(this.Cars[i]);\r\n            }\r\n          }\r\n        }\r\n\r\n        if(!this.vybraneKaroserie){\r\n          for(let i =0; i<this.vybranaAuta.length; i++){\r\n            if (!(this.vybraneKaroserie.includes(this.vybranaAuta[i].karoserie))) {\r\n              this.vybranaAuta.splice(i, 1);\r\n              i =-1\r\n            }\r\n          }\r\n\r\n\r\n\r\n          for(let i =0; i<this.Cars.length; i++) {\r\n            if (this.vybraneKaroserie.includes(this.Cars[i].karoserie) && (!this.isInArray(this.Cars[i]._id)) ) {\r\n              this.vybranaAuta.push(this.Cars[i]);\r\n            }\r\n          }\r\n        }\r\n\r\n\r\n    },\r\n\r\n\r\n    toCarDetail(car){\r\n      this.zvoleneAuto = car;\r\n      this.$router.push('./carDetail');\r\n\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.border-text-choose-car{\r\n  text-align: center;\r\n  width: 75%;\r\n\r\n  background: #ffee80;\r\n  font-size: 20px;\r\n  border-radius: 1em;\r\n\r\n\r\n  margin-bottom: 5rem;\r\n}\r\n.carDescription{\r\n  position: relative;\r\n  right: -20rem;\r\n}\r\n\r\n\r\n\r\n.border-title-choose-car{\r\n  font-size: 25px;\r\n  color: black;\r\n  font-weight: revert;\r\n  margin-right: 50em;\r\n\r\n}\r\n\r\n.filtering-tag {\r\n  align-items: center;\r\n  width: 100%;\r\n\r\n\r\n  margin-bottom: 6em;\r\n  margin-top: 12em;\r\n  z-index: 1000;\r\n  background: floralwhite;\r\n  border-bottom: inset #130f40;\r\n\r\n}\r\n\r\n\r\n.filter-znacky{\r\n  column-count: 2;\r\n  padding-right: -15rem;\r\n}\r\n\r\n.imgCar{\r\n  padding-right: 1em;\r\n}\r\n.showCar{\r\n  background-color: #008CBA;\r\n  border: none;\r\n  color: white;\r\n  padding: 15px 32px;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  font-size: 16px;\r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}