Posts();\r\n    }catch (err){\r\n      this.error =err.message;\r\n    }\r\n  }\r\n}\r\n*/\r\nexport default {\r\n  name: 'PostComponents',\r\n  data() {\r\n    return {\r\n      User: {},\r\n    }\r\n  },\r\n  mounted() {\r\n    axios.get('http://localhost:3000/')\r\n        .then((response) =>{\r\n          console.log(response.data);\r\n          this.User = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}